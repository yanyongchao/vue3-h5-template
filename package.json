{
  "name": "vue3-h5-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "build:test": "vue-cli-service build --mode test",
    "build:beta": "vue-cli-service build --mode beta",
    "build:dll": "webpack --config webpack.dll.conf.js",
    "report": "vue-cli-service build --report --mode production",
    "lint": "vue-cli-service lint"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "stylelint --fix"
    ],
    "*.css": [
      "stylelint --fix"
    ],
    "*.less": [
      "stylelint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "sideEffects": [
    "*.css",
    "*.vue"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "dependencies": {
    "async-validator": "^3.5.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "vant": "^3.0.4",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "5.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "node-sass": "^4.12.0",
    "postcss-px-to-viewport": "^1.1.1",
    "sass-loader": "^8.0.2",
    "stylelint-webpack-plugin": "^2.1.1",
    "typescript": "~3.9.3",
    "useless-files-webpack-plugin": "^1.0.1",
    "validate-commit-msg": "^2.14.0",
    "vconsole": "^3.4.0",
    "vconsole-webpack-plugin": "^1.5.2",
    "vue-cli-plugin-fs-stylelint": "^0.1.2",
    "webpack-cli": "^4.4.0"
  }
}
